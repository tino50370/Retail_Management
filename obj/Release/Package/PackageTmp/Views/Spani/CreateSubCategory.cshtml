@model RetailKing.Models.JobSubCategory


@{
    ViewBag.Title = "Create";
}
<div class="row">
    <div class="col-md-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>New Job SubCategory </h2>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div class="row">

                    @using (Html.BeginForm("CreateSubCategory", "Spani", FormMethod.Post, new { id = "createSubCategory" }))
                    {
                        @Html.ValidationSummary(true)
                        <fieldset>
                           

                            @Html.Partial("_CreateOrEditSubCategory", Model)

                        </fieldset>
                    }
                    <br/>
                    <div>
                        <button class="btn btn-success" onclick="SaveView('createSubCategory')">Save</button>
                        <button class="btn btn-warning" onclick="GetView('@Url.Action("Index", "Spani")')">Cancel</button>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
