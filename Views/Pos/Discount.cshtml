@{
    ViewBag.Title = "Discount";
}


<div class="container_12" style="width:500px">
    <div class="x_panel">
        <div class="row">
            <div class="x_content">
                <h3>@ViewData["item"]</h3>
                <h4>You are about to Discount @ViewData["disType"] on this item</h4>
                <br />
                <button type="button" onclick="discountYes('@ViewData["id"]','@ViewData["item"]','@ViewData["prize"]','@ViewData["type"]', '@ViewData["Qty"]', '@ViewData["dcount"]','@ViewData["tax"]')" class="btn btn-success"> Yes</button>
                <button type="button" onclick="discountNo('@ViewData["id"]','@ViewData["item"]','@ViewData["prize"]','@ViewData["type"]', '@ViewData["Qty"]', '@ViewData["dcount"]','@ViewData["tax"]')" class="btn btn-warning"> No</button>
            </div>
        </div>
    </div>
</div>
