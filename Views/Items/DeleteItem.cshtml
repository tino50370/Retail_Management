@{
    ViewBag.Title = "DeleteItem";
}


<div class="container_12" style="width:500px">
    <div class="x_panel">
        <div class="row">
            <div class="x_content">
                <h3>@ViewData["item"]</h3>
                <h4>You are about to Delete this item from the inventory list</h4>
                <br />
                <button type="button" onclick="DeleteItem('@ViewData["ID"]','@ViewData["currentPage"]')" class="btn btn-success"> Yes</button>
                <button type="button" onclick="CloseCancel()" class="btn btn-warning"> No</button>
            </div>
        </div>
    </div>
</div>
