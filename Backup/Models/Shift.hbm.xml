<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">

  <class name="RetailKing.Models.Shift, RetailKing" table="shift" >

    <id name="Id" column="ID" type="System.Int64" unsaved-value="0">
      <generator class="native"/>
    </id>

    <property name="ShiftNumber" column="ShiftNumber"  not-null="false" type="System.Int64"   insert="true" update="true"/>
    <property name="Manager" column="Manager"  not-null="false" type="System.String"  length="100" insert="true" update="true"/>
    <property name="State" column="State"  not-null="false" type="System.String"  length="100" insert="true" update="true"/>
    <property name="Dated" column="dated"  not-null="false" type="System.String"  length="100" insert="true" update="true"/>
   
   
  </class>
</hibernate-mapping>
