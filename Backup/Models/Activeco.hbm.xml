<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">

  <class name="RetailKing.Models.Activeco, RetailKing" table="Activeco" >

    <id name="Id" column="ID" type="System.Int64" unsaved-value="0">
      <generator class="native"/>
    </id>

    <property name="company" column="company"  not-null="false" type="System.String"  length="100" insert="true" update="true"/>
    <property name="monthlyTarget" column="monthlyTarget"  not-null="false" type="System.Decimal"  length="40" insert="true" update="true"/>
    <property name="dailyTarget" column="dailyTarget"  not-null="false" type="System.Decimal"  length="40" insert="true" update="true"/>

   
  </class>
</hibernate-mapping>
